'         HWRITE      == 0;',
' 	 hburst_type == hburst_enum\'(k);',
'	 hsize_type  == hsize_enum\'(i);',
'	 incr_size   == 3;',
'	 if(beat_cnt!=1) busy_trans_cycles[0]==16;',
'       })',
'',
'    end',
'  end',
'',
'  `uvm_info("body", "Exiting ...", UVM_MEDIUM)',
'',
'endtask : body',
